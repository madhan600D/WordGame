Console:

<BackgroundColorLayout>:
    canvas.before:
        Color:
            rgba: 0.3, 0.5, 0.8, .2  # Set the RGBA values of the desired color
        Rectangle:
            pos: self.pos
            size: self.size
<console>:
    orientation:"vertical"
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1)
        Rectangle:
            source:'Background image.jfif'
            size: root.width, root.height
            pos: self.pos

    BoxLayout:
        orientation:"vertical"
            
        BoxLayout:
            size_hint:1,.2
            Label:
                text:"WORD HUNT"
                font_name:"lcd"
                color:1,0,0,1
                font_size:"400sp"
                opacity:1
        BoxLayout:
            size_hint:1,.2
            RelativeLayout:
                Label:
                    id:chance_display
                    font_name: 'lcd'
                    color: 1, 1, 0, 1
                    size_hint: None, None
                    size: self.texture_size
                    font_size: '100sp'
                    opacity: 1
                    pos_hint: {'right': .9, 'center_y': 0.5}
        BoxLayout:
            orientation:"vertical"
            Label:
                text:"Guess the word"
                font_name:"lcd"
                color:1,0,0,1
                font_size:"100sp"
                opacity:1
            Label:
                text:""
                id:display_label
                font_name:"lcd"
                color:1,0,0,1
                font_size:"100sp"
                opacity:1
            Label:
                text:""
                id:hint_label
                font_name:"lcd"
                color:1,0,0,1
                font_size:"100sp"
                opacity:1
        GridLayout:
            size_hint:1,.45
            cols:11
            rows:4
            Button_animate:
                text:"Q"
                font_name:"3D"
                on_press:self.on_press()
                on_release:self.on_release(),root.game('q')
                #on_complete:self.reset_button()
                background_color:(0,0,0,.4)
                border:(6,6,6,6)
                border_color:(1,0,0,1)
                font_size:'30sp'
                color:1,.6, 0,.8
                canvas.before:
                    Color:
                        rgba:1,.6, 0,.8  # Yellow border color
                        #gradient
                    Line:
                        width: .5  # Border width
                        rectangle: self.x, self.y, self.width, self.height
                #padding_right:
